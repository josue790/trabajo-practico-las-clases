class Rectangulo:
     def __init__(self, base, altura):
         self._base = base
         self._altura = altura
     def area(self):
         return self._base * self._altura
     def perimetro(self):
         return 2 * (self._base + self._altura)
 if __name__ == "__main__":
     rect = Rectangulo(2, 12)
     print("El Área del rectangulo es:", rect.area())
     print("El Perímetro del rectangulo es :", rect.perimetro())
