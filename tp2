class Persona:
     def __init__(self, nombre, edad, sexo):
         self.nombre = nombre
         self.edad = edad
         self.sexo = sexo
     def cambiar_edad(self, nueva_edad):
         self.edad = nueva_edad
     def mostrar_informacion(self):
         return f"Nombre: {self.nombre}, Edad: {self.edad}, Sexo: {self.sexo}"
 if __name__ == "__main__":
     persona1 = Persona("Ana", 30, "Femenino")
     persona1.cambiar_edad(35)
     print(persona1.mostrar_informacion())
